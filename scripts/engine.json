{
	"product" : "FabMo-Engine",
	"os" : "linux",
	"platform" : "raspberry-pi",
	"system" : "dt",
	"token" : "/fabmo/install_token",
	"repository" : "https://github.com/FabMo/FabMo-Engine",
	"updaterNeeded" : "v2.0.0",
	"services" : ["fabmo"],
	"operations" : [
		{
			"op" : "deleteFiles",
			"paths" : [
				"/fabmo"
			]
		},
		{
			"op" : "expandArchive",
			"src" : "files.tar.gz",
			"dest" : "/fabmo"
		},
		{
            "op" : "installFirmware",
            "src" : "g2core_101.57.28build_FabMo-G2.bin"
       },
       {
            "op" : "deleteFiles",
            "paths" : [
                "/opt/fabmo/approot",
        		"/opt/fabmo/profiles"
            ]
        }
	]
}
