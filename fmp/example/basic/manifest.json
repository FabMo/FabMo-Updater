{
	"product" : "FabMo-Engine",
	"repository" : "https://github.com/FabMo/FabMo-Engine",
	"os" : "*",
	"platform" : "*",
	"version" : "v99.99.99",
	"services" : ["fabmo"],
	"updaterNeeded" : "1.4.0",
	"token" : "/tmp/.update_token",
	"operations" : [
		{
			"op" : "deleteFiles",
			"paths" : [
				"/fabmo/engine"
			]
		},
		{
			"op" : "expandArchive",
			"src" : "dummy.tar.gz",
			"dest" : "/fabmo/engine"
		},
		{
		    "op" : "installFirmware",
		    "src" : "g2.bin"
		}
	]
}
